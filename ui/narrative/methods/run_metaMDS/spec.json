{
    "ver": "0.0.1",
    "authors": [
        "qzhang", "weise", "james_verasoto", "tgu2"
    ],
    "contact": "http://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": "kbaseNarrativeMethodInput",
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_obj_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseMatrices"]
            }
        },
        {
            "id" : "dimension",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : ["col"],
            "field_type" : "dropdown",
            "dropdown_options":
            {
                "options":
                [
                {
                    "display": "Row",
                    "value": "row"
                },
                {
                    "display": "Column",
                    "value": "col"
                }
                ]
        }
        },
        {
            "id" : "n_components",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "2" ],
            "field_type" : "text",
            "text_options" : {
	        "validate_as": "int"
            }
        },
        {
            "id" : "max_iter",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "300" ],
            "field_type" : "text",
            "text_options" : {
	        "validate_as": "int"
            }
        },
        {
            "id" : "mds_matrix_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
              "valid_ws_types" : ["KBaseExperiments.PCAMatrix"],
              "is_output_name" : true
            }
        },
        {
          "id" : "attribute_mapping_obj_ref",
          "optional" : true,
          "advanced" : false,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "text",
          "text_options" : {
            "valid_ws_types" : ["KBaseExperiments.AttributeMapping"]
          }
        },
        {
          "id" : "associated_matrix_obj_ref",
          "optional" : true,
          "advanced" : false,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "text",
          "text_options" : {
            "valid_ws_types" : ["KBaseMatrices"]
          }
        },
        {
          "id" : "attribute_color",
          "optional" : false,
          "advanced" : true,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "textsubdata",
          "textsubdata_options" : {
            "subdata_selection": {
              "parameter_id" : "attribute_mapping_obj_ref",
              "subdata_included" : ["attributes/[*]/attribute"],
              "path_to_subdata": ["attributes"],
              "selection_id": "attribute"
              },
            "multiselection":false,
            "show_src_obj":false,
            "allow_custom":false
          }
        },
        {
          "id" : "attribute_size",
          "optional" : true,
          "advanced" : true,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "textsubdata",
          "textsubdata_options" : {
            "subdata_selection": {
              "parameter_id" : "attribute_mapping_obj_ref",
              "subdata_included" : ["attributes/[*]/attribute"],
              "path_to_subdata": ["attributes"],
              "selection_id": "attribute"
              },
            "multiselection":false,
            "show_src_obj":false,
            "allow_custom":false
          }
        },
        {
          "id" : "row_size",
          "optional" : true,
          "advanced" : true,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "textsubdata",
          "textsubdata_options" : {
            "subdata_selection": {
              "parameter_id" : "associated_matrix_obj_ref",
              "subdata_included" : ["data/row_ids"],
              "path_to_subdata": ["data", "row_ids"],
              "selection_id": "id"
              },
            "multiselection":false,
            "show_src_obj":false,
            "allow_custom":false
          }
        },
        {
          "id" : "highlight_row",
          "optional" : true,
          "advanced" : true,
          "allow_multiple" : true,
          "default_values" : [ "" ],
          "field_type" : "textsubdata",
          "textsubdata_options" : {
            "subdata_selection": {
              "parameter_id" : "associated_matrix_obj_ref",
              "subdata_included" : ["data/row_ids"],
              "path_to_subdata": ["data", "row_ids"],
              "selection_id": "id"
              },
            "multiselection":false,
            "show_src_obj":false,
            "allow_custom":false
          }
        },
        {
          "id" : "col_size",
          "optional" : true,
          "advanced" : true,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "textsubdata",
          "textsubdata_options" : {
            "subdata_selection": {
              "parameter_id" : "associated_matrix_obj_ref",
              "subdata_included" : ["data/col_ids"],
              "path_to_subdata": ["data", "col_ids"],
              "selection_id": "id"
              },
            "multiselection":false,
            "show_src_obj":false,
            "allow_custom":false
          }
        },
        {
          "id" : "highlight_col",
          "optional" : true,
          "advanced" : true,
          "allow_multiple" : true,
          "default_values" : [ "" ],
          "field_type" : "textsubdata",
          "textsubdata_options" : {
            "subdata_selection": {
              "parameter_id" : "associated_matrix_obj_ref",
              "subdata_included" : ["data/col_ids"],
              "path_to_subdata": ["data", "col_ids"],
              "selection_id": "id"
              },
            "multiselection":false,
            "show_src_obj":false,
            "allow_custom":false
          }
        },
        {
          "id": "only_highlight",
          "optional":false,
          "advanced":true,
          "allow_multiple":false,
          "default_values":["1"],
          "field_type" : "checkbox",
          "checkbox_options":{
            "checked_value": 1,
            "unchecked_value": 0
          }
    }
    ],
    "parameter-groups": [
    {
      "id": "color_marker_by",
      "parameters": [
        "attribute_color"
      ],
      "optional": true,
      "allow_multiple": false,
      "with_border": false,
      "advanced" : false
    },
    {
      "id": "scale_size_by",
      "parameters": [
        "attribute_size",
        "row_size",
        "highlight_row",
        "col_size",
        "highlight_col"
      ],
      "optional": true,
      "allow_multiple": false,
      "with_border": false,
      "advanced" : false
    }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_Amplicon",
            "method": "run_metaMDS",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "input_parameter": "input_obj_ref",
                    "target_property": "input_obj_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter" : "attribute_mapping_obj_ref",
                    "target_property" : "attribute_mapping_obj_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter" : "associated_matrix_obj_ref",
                    "target_property" : "associated_matrix_obj_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter" : "n_components",
                    "target_property" : "n_components"
                },{
                    "input_parameter" : "dimension",
                    "target_property" : "dimension"
                },{
                    "input_parameter" : "max_iter",
                    "target_property" : "max_iter"
                },{
                    "input_parameter" : "scale_size_by",
                    "target_property" : "scale_size_by"
                },{
                    "input_parameter" : "color_marker_by",
                    "target_property" : "color_marker_by"
                },{
                    "input_parameter" : "mds_matrix_name",
                    "target_property" : "mds_matrix_name"
                },{
                    "constant_value" : "plot(mds_data.mds)",
                    "target_property" : "plot_script"
                },{
                    "constant_value" : "pdf",
                    "target_property" : "plot_type"
                },{
                    "constant_value" : "usrplot",
                    "target_property" : "plot_name"
                },{
                    "input_parameter" : "only_highlight",
                    "target_property" : "only_highlight"
                }
            ],
            "output_mapping" : [
                {
                "narrative_system_variable": "workspace",
                "target_property": "wsName"
                },{
                    "service_method_output_path": [0, "mds_ref"],
                    "target_property": "mds_ref"
                },{
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },{
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
	        ]
        }
    },
    "job_id_output_field": "docker"
}
